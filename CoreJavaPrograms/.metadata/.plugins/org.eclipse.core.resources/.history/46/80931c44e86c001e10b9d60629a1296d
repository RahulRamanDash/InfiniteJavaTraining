package com.java.day4;

import java.util.ArrayList;
import java.util.List;

public class ArrayAndArrayList {
	public static void main(String[] args) {
		int[] arr = {1,2,3,4,5};
		
		
		for (int i : arr) {
			System.out.println(i);
		}
		
		List<Integer> arr1 = new ArrayList<>();
		
		arr1.add(null)
		
		
	}

}
