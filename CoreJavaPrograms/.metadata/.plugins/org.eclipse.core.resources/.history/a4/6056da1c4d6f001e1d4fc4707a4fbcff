package com.java.day8;

public class ArraySort {
	public static void main(String[] args) {
		 int arr[] = {12, 13, 1, 54, 34, 10};
		 
		 for (int i = 0; i<arr.length-1;i++) {
			 if(arr[i]>arr[i+1]) {
			
				 int temp = arr[i];
				 arr[i] = arr[i+1];
				 arr[i+1] = temp;
			 }
		 }
		 
		 for(int j = 1; j<arr.length;j++) {
			 System.out.println(arr[j]);
		 }
	}
}
