package com.java.day7;

public class ThreadPriorityExample {
    public static void main(String[] args) {
        Thread t1 = new Thread(() -> {
            for (int i = 1; i <= 5; i++) {
                System.out.println("Thread 1: " + i);
            }
        });
        Thread t2 = new Thread(() -> {
            for (int i = 1; i <= 5; i++) {
                System.out.println("Thread 2: " + i);
            }
        });

        t1.setPriority(Thread.MIN_PRIORITY); // Set thread 1's priority to the minimum
        t2.setPriority(Thread.MAX_PRIORITY); // Set thread 2's priority to the maximum

        t1.start();
        t2.start();
    }
}
