
public class CountString {

	public static void main(String[] args) {
		String name = "Rahul Raman Dash";
		int count=0; 
		Integer[] list;
		for(int i=1;i<name.length();i++) {
			if(name.charAt(i)=='a') {
				count+=1;
			}
		}
		System.out.println(count);
		System.out.println("a is at index : "+);
	}

}
